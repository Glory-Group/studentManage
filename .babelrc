{
    "presets": [
        "react-app"
      ],
    "plugins": [
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy":true
            }
        ],
        //做antd按需加载
        ["import", {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": "css" // `style: true` 会加载 less 文件
        }]
      ]
}